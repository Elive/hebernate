#!/bin/bash
SOURCE="$0"
source /usr/lib/elive-tools/functions
REPORTS="1"
el_make_environment
. gettext.sh
TEXTDOMAIN="hebernate"
export TEXTDOMAIN

main(){
    # pre {{{
    local arg

    # }}}

    for arg in "$@"
    do
        case "$arg" in
            suspend)
                is_suspend=1
                ;;
        esac
    done

}

#
#  MAIN
#
main "$@"

